{
  "name": "sharkie",
  "displayName": "Sharkie",
  "version": "0.0.0",
  "description": "A basic Plasmo extension.",
  "author": "hp",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build"
  },
  "dependencies": {
    "@tailwindcss/forms": "0.5.3",
    "@tailwindcss/typography": "0.5.7",
    "axios": "^0.27.2",
    "bootstrap": "^5.2.1",
    "brain.js": "2.0.0-beta.16",
    "cohere-ai": "^4.3.1",
    "docx": "^7.5.0",
    "file-saver": "^2.0.5",
    "https-browserify": "^1.0.0",
    "querystring-es3": "^0.2.1",
    "react": "18.2.0",
    "react-bootstrap": "^2.5.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.4.0",
    "stream-http": "^3.2.0",
    "tailwindcss": "3.1.8",
    "url": "^0.11.0",
    "wikipedia-tldr": "^2.0.1"
  },
  "devDependencies": {
    "@plasmohq/prettier-plugin-sort-imports": "3.5.2",
    "@types/chrome": "0.0.197",
    "@types/node": "18.7.22",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "plasmo": "0.55.0",
    "postcss": "8.4.16",
    "prettier": "2.7.1",
    "typescript": "4.8.3"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ],
    "permissions": [
      "contextMenus",
      "tabs"
    ],
    "web_accessible_resources": [
      {
        "matches": [
          "<all_urls>"
        ],
        "resources": [
          "sharks/*.jpg"
        ]
      }
    ]
  }
}
